<h1>Specific year passing through</h1>

<div class="columns">
  <div class="column">
    <div class="field is-horizontal">
      <div class="field-label">
        <label class="label">Month</label>
      </div>
      <div class="field-body">
        <div class="field">
          {{#each allMonths as |month|}}
            <button class="button is-light mb-1">{{month}}</button>
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</div>
{{outlet}}